{
    "name": "weblumtest",
    "share_target": {
        "action": "/upload/",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "jpeg",
                    "accept": ["image/jpeg", ".jpeg"]
                },
                {
                    "name": "jpg",
                    "accept": ["image/jpeg", ".jpg"]
                },
                {
                    "name": "png",
                    "accept": ["image/png", ".png"]
                }
            ]
        }
    },
    "icons": [
        {
            "src": "/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "display": "standalone",
    "start_url": "https://app.weblum.photos/"
}